{
  "name": "trim-whitespace",
  "version": "1.3.1",
  "description": "Remove leading and/or trailing spaces and tabs from each line of a string",
  "engines": {
    "node": "^7.8.0",
    "npm": "^4.5.0"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix index.js spec/",
    "test": "jest && npm run lint",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "leading",
    "trailing",
    "trim",
    "whitespace"
  ],
  "author": "Benjamin Marinacci",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcmarinacci/trim-whitespace.git"
  },
  "bugs": {
    "url": "https://github.com/bcmarinacci/trim-whitespace/issues"
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "jest": "^19.0.2"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/coverage/",
      "<rootDir>/node_modules/"
    ]
  }
}
