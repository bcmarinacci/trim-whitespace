{
  "name": "trim-whitespace",
  "version": "1.3.1",
  "description": "Remove leading and/or trailing spaces and tabs from each line of a string",
  "engines": {
    "node": "^7.8.0",
    "npm": "^4.5.0"
  },
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --single-quote --write index.js __tests__/**/*.js",
    "test": "jest",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "leading",
    "trailing",
    "trim",
    "whitespace"
  ],
  "author": "Benjamin Marinacci",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcmarinacci/trim-whitespace.git"
  },
  "bugs": {
    "url": "https://github.com/bcmarinacci/trim-whitespace/issues"
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "coveralls": "^2.11.9",
    "jest": "^19.0.2",
    "prettier": "^1.1.0"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/coverage/",
      "<rootDir>/node_modules/"
    ]
  }
}
